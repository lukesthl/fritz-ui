{
  "name": "fritz-ui",
  "version": "1.1.7",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "semantic-release": "semantic-release"
  },
  "author": {
    "name": "Luke",
    "email": "lukesthl@users.noreply.github.com",
    "url": "https://github.com/lukesthl"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@codedependant/semantic-release-docker": "^5.1.1",
    "@heroicons/react": "^2.2.0",
    "@lukesthl/fritzbox": "^1.0.2",
    "@lukesthl/ts-axios-digest-auth": "^1.0.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tabs": "^1.1.13",
    "@remixicon/react": "^4.6.0",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@total-typescript/ts-reset": "^0.6.1",
    "@trpc/client": "^11.4.4",
    "@trpc/next": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "^15.4.6",
    "form-data": "^4.0.4",
    "formik": "^2.4.6",
    "framer-motion": "^12.23.12",
    "fuse.js": "^7.1.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.9",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "^6.0.0",
    "recharts": "^2.15.3",
    "semantic-release": "^24.2.7",
    "superjson": "2.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "xmlbuilder": "^15.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/node": "^22.15.27",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@codedependant/semantic-release-docker",
        {
          "dockerRegistry": "ghcr.io",
          "dockerLogin": false,
          "dockerProject": "lukesthl",
          "dockerTags": [
            "{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
            "{{major}}-{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
            "{{major}}.{{minor}}-{{#if prerelease.[0]}}{{prerelease.[0]}}{{else}}latest{{/if}}",
            "{{version}}"
          ],
          "dockerPlatform": [
            "linux/amd64",
            "linux/arm64"
          ],
          "dockerArgs": {
            "GITHUB_TOKEN": true
          }
        }
      ]
    ]
  },
  "bugs": {
    "url": "https://github.com/lukesthl/fritz-ui/issues"
  },
  "homepage": "https://github.com/lukesthl/fritz-ui#readme"
}
